local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local StarterGui = game:GetService("StarterGui")
local LocalPlayer = Players.LocalPlayer

local function sendNotification(title, text, icon)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Icon = icon,
            Duration = 5
        })
    end)
end

sendNotification("Thank You!", "Thank you for using cOOLouie FE Executor. Enjoy!", "")

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game:GetService("CoreGui")

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 350, 0, 400)
Frame.Position = UDim2.new(0.5, -175, 0.5, -200)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BackgroundTransparency = 0.1
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui
Frame.Draggable = true
Frame.Active = true
Frame.ClipsDescendants = true
Frame.Visible = true

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0.3, 0)
UICorner.Parent = Frame

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0.12, 0)
Title.Position = UDim2.new(0, 0, 0, 0)
Title.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Title.Text = "cOOLouie FE Executor"
Title.TextSize = 20
Title.Font = Enum.Font.SourceSansBold
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextStrokeTransparency = 0.5
Title.TextScaled = true
Title.Parent = Frame

local TitleCorner = Instance.new("UICorner")
TitleCorner.CornerRadius = UDim.new(0.3, 0)
TitleCorner.Parent = Title

spawn(function()
    while true do
        for i = 0, 255, 5 do
            Title.TextColor3 = Color3.fromHSV(i / 255, 1, 1)
            wait(0.1)
        end
    end
end)

local TabsFrame = Instance.new("Frame")
TabsFrame.Size = UDim2.new(1, 0, 0.1, 0)
TabsFrame.Position = UDim2.new(0, 0, 0.12, 0)
TabsFrame.BackgroundTransparency = 1
TabsFrame.Parent = Frame

local function createTab(name, position)
    local Tab = Instance.new("TextButton")
    Tab.Size = UDim2.new(0.33, -5, 1, 0)
    Tab.Position = UDim2.new(position, 0, 0, 0)
    Tab.Text = name
    Tab.Parent = TabsFrame
    
    local TabCorner = Instance.new("UICorner")
    TabCorner.CornerRadius = UDim.new(0.3, 0)
    TabCorner.Parent = Tab
    
    return Tab
end

local ExecutorTab = createTab("Executor", 0)
local PlayerInfoTab = createTab("Player Info", 0.33)
local ScriptHubTab = createTab("Script Hub", 0.66)

local ExecutorBox = Instance.new("TextBox")
ExecutorBox.Size = UDim2.new(1, -10, 0.5, 0)
ExecutorBox.Position = UDim2.new(0, 5, 0.25, 0)
ExecutorBox.ClearTextOnFocus = false
ExecutorBox.Text = "-- Paste script here"
ExecutorBox.Parent = Frame

local ExecuteButton = Instance.new("TextButton")
ExecuteButton.Size = UDim2.new(0.48, -5, 0.1, 0)
ExecuteButton.Position = UDim2.new(0, 5, 0.78, 0)
ExecuteButton.Text = "Execute"
ExecuteButton.Parent = Frame

local ClearButton = Instance.new("TextButton")
ClearButton.Size = UDim2.new(0.48, -5, 0.1, 0)
ClearButton.Position = UDim2.new(0.52, 0, 0.78, 0)
ClearButton.Text = "Clear"
ClearButton.Parent = Frame
ClearButton.MouseButton1Click:Connect(function()
    ExecutorBox.Text = ""
end)

local PlayerInfo = Instance.new("TextLabel")
PlayerInfo.Size = UDim2.new(1, -10, 0.5, 0)
PlayerInfo.Position = UDim2.new(0, 5, 0.25, 0)
PlayerInfo.Text = "Fetching Player Info..."
PlayerInfo.Visible = false
PlayerInfo.Parent = Frame

local ScriptHub = Instance.new("Frame")
ScriptHub.Size = UDim2.new(1, -10, 0.5, 0)
ScriptHub.Position = UDim2.new(0, 5, 0.25, 0)
ScriptHub.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
ScriptHub.Visible = false
ScriptHub.Parent = Frame

local AddScriptButton = Instance.new("TextButton")
AddScriptButton.Size = UDim2.new(1, -10, 0.1, 0)
AddScriptButton.Position = UDim2.new(0, 5, 0.7, 0)
AddScriptButton.Text = "Add Script"
AddScriptButton.Parent = ScriptHub
local notified = false
AddScriptButton.MouseButton1Click:Connect(function()
    if not notified then
        sendNotification("Script Hub", "Feature under development!", "")
        notified = true
    end
end)

ExecutorTab.MouseButton1Click:Connect(function()
    ExecutorBox.Visible = true
    ExecuteButton.Visible = true
    ClearButton.Visible = true
    PlayerInfo.Visible = false
    ScriptHub.Visible = false
end)

PlayerInfoTab.MouseButton1Click:Connect(function()
    ExecutorBox.Visible = false
    ExecuteButton.Visible = false
    ClearButton.Visible = false
    PlayerInfo.Visible = true
    ScriptHub.Visible = false
end)

ScriptHubTab.MouseButton1Click:Connect(function()
    ExecutorBox.Visible = false
    ExecuteButton.Visible = false
    ClearButton.Visible = false
    PlayerInfo.Visible = false
    ScriptHub.Visible = true
end)

local HideButton = Instance.new("TextButton")
HideButton.Size = UDim2.new(0.2, 0, 0.08, 0)
HideButton.Position = UDim2.new(0.8, 0, 0.92, 0)
HideButton.Text = "Hide"
HideButton.Parent = Frame

HideButton.MouseButton1Click:Connect(function()
    Frame.Visible = not Frame.Visible
end)

sendNotification("FE Bypass", "Injecting FE Bypass...", "")
